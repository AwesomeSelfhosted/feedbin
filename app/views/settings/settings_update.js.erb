$('[data-behavior~=flash_wrap]').html('<%= j render partial: 'shared/flash', locals: {flash: flash} %>')

clearTimeout(clearMessage);
var clearMessage = setTimeout(function() {
    $('[data-behavior~=flash_wrap]').html('');
}, 5000)

<% if @user.setting_on?(:starred_feed_enabled) %>
    $('[data-behavior~=starred_feed_url]').html('<%= j render partial: 'shared/starred_feed_url', locals: {user: @user} %>')
<% else %>
    $('[data-behavior~=starred_feed_url]').html('')
<% end %>
